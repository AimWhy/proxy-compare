{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "es2015",
    "moduleResolution": "node",
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "./dist"
  }
}
